{
  "name": "@saber2pr/ts-compiler",
  "version": "0.3.1",
  "description": "ts compiler utils.",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Saber2pr/ts-compiler.git"
  },
  "author": "saber2pr",
  "license": "ISC",
  "files": [
    "lib"
  ],
  "main": "./lib/index.js",
  "scripts": {
    "test": "jest src/__tests__",
    "start": "tsc --watch",
    "prepublishOnly": "tsc",
    "release": "standard-version",
    "lint": "prettier --write ./src",
    "docs": "typedoc src/index.ts",
    "gen:finder": "ts-node src/utils/scripts/code-gen-finder",
    "prepare": "husky install"
  },
  "dependencies": {
    "@nodelib/fs.walk": "^1.2.8",
    "@types/graceful-fs": "^4.1.5",
    "@types/node": "^16.3.3",
    "graceful-fs": "^4.2.8",
    "typescript": "^4.3.5"
  },
  "devDependencies": {
    "@types/jest": "^24.0.12",
    "husky": ">=6",
    "jest": "^24.8.0",
    "lint-staged": ">=10",
    "prettier": "^2.4.1",
    "standard-version": "^9.3.1",
    "ts-jest": "^24.0.2",
    "ts-node": "^10.2.1",
    "typedoc": "^0.21.5"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "yarn lint",
      "git add ."
    ]
  }
}
